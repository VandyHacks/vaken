	"scripts": {
		"server:start": "ts-node -r dotenv/config src/server/index.ts",
		"server:dev": "ts-node-dev -r dotenv/config --respawn --transpile-only ./src/server/index.ts",
		"server:dev:debug": "ts-node-dev -r dotenv/config --respawn --inspect --transpile-only ./src/server/index.ts",
		"client:dev": "webpack-dev-server --config webpack.dev.js --progress --color",
		"build": "concurrently \"tsc\" \"webpack --config webpack.prod.js --progress --colors\"",
		"prod": "npm run build && node ./dist/server/index.js",
		"format": "prettier --config .prettierrc.json --write \"./**/*.ts*(x)\"",
		"start": "concurrently -n client,server \"npm:client:dev\" \"npm:server:dev:debug\"",
		"test": "jest --onlyChanged",
		"test-ci": "jest --ci --runInBand",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"docs": "typedoc",
		"lint-check": "eslint --cache --report-unused-disable-directives --ext .js,.jsx,.ts,.tsx __tests__/ src/",
		"lint-fix": "eslint --cache --report-unused-disable-directives --fix --ext .js,.jsx,.ts,.tsx __tests__/ src/",
		"lint-ci": "eslint --ext .js,.jsx,.ts,.tsx __tests__/ src/ --format junit --output-file ./reports/lint/eslint/results.xml",
		"generate": "graphql-codegen --config codegen.json",
		"prestart": "npm run generate",
		"prestart-server:start": "npm run generate"
	},
