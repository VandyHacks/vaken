{
  "name": "vaken",
  "version": "1.0.0",
  "description": "Next-gen hacker registration system",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "format": "prettier --parser typescript --single-quote --trailing-comma=es5 --write \"./**/*.ts\"",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "start": "ts-node src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc && npm run lint && npm run format",
      "pre-push": "tsc && npm run lint && npm run format"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VandyHacks/vaken.git"
  },
  "keywords": [
    "hackathon",
    "registration"
  ],
  "author": "VandyHacks",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VandyHacks/vaken/issues"
  },
  "homepage": "https://github.com/VandyHacks/vaken#readme",
  "dependencies": {
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "husky": "^1.2.0",
    "nodemon": "^1.18.7",
    "prettier": "^1.15.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  }
}
