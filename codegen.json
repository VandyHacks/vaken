{
	"overwrite": true,
	"watch": false,
	"generates": {
		"services/users/lib/generated.graphql.ts": {
			"schema": ["./services/users/users.graphql.ts"],
			"plugins": ["typescript", "typescript-resolvers", "typescript-mongodb"],
			"config": {
				"federation": true
			}
		},
		"services/resumes/lib/generated.graphql.ts": {
			"schema": ["./services/resumes/resumes.graphql.ts"],
			"plugins": ["typescript", "typescript-resolvers", "typescript-mongodb"],
			"config": {
				"federation": true
			}
		},
		"services/groups/lib/generated.graphql.ts": {
			"schema": ["./services/groups/groups.graphql.ts"],
			"plugins": ["typescript", "typescript-resolvers", "typescript-mongodb"],
			"config": {
				"federation": true
			}
		},
		"common/types/generated.ts": {
			"schema": ["./services/**/*.graphql*"],
			"plugins": ["typescript"]
		},
		"services/gateway/auth/generated.graphql.ts": {
			"schema": ["./services/**/*.graphql.ts"],
			"documents": ["./services/gateway/**/login.graphql.ts"],
			"plugins": ["typescript", "typescript-operations", "typed-document-node"]
		},
		"services/groups/lib/query/generated.graphql.ts": {
			"schema": ["./services/**/*.graphql.ts"],
			"documents": ["./services/groups/lib/query/*query.graphql.ts"],
			"plugins": ["typescript", "typescript-operations", "typed-document-node"]
		},
		"services/resumes/lib/query/generated.graphql.ts": {
			"schema": ["./services/**/*.graphql.ts"],
			"documents": ["./services/resumes/lib/query/*query.graphql.ts"],
			"plugins": ["typescript", "typescript-operations", "typed-document-node"]
		},
		"src/client/generated/graphql.ts": {
			"schema": ["./services/**/*.graphql*"],
			"plugins": [
				"typescript",
				"typescript-operations",
				"typescript-mongodb",
				"typescript-react-apollo"
			],
			"config": {
				"withHOC": false,
				"withComponent": false,
				"withHooks": true,
				"hooksImportFrom": "@apollo/react-hooks",
				"withMutationFn": false
			}
		}
	}
}
